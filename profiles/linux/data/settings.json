{
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "[toml]": {
        "editor.defaultFormatter": "tombi-toml.tombi"
    },
    "clangd.path": "/home/conner/.config/VSCodium/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/19.1.2/clangd_19.1.2/bin/clangd",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.formatOnSave": false,
    "editor.minimap.enabled": false,
    "files.associations": {
        "**/handlers/**/**/*.yml": "ansible",
        "**/playbooks/*.yml": "ansible",
        "**/playbooks/tasks/*.yml": "ansible",
        "**/roles/**/**/*.yml": "ansible",
        "*.log.*": "log",
        ".rc": "shellscript"
    },
    "files.insertFinalNewline": true,
    "git.enableCommitSigning": true,
    "git.inputValidation": true,
    "git.suggestSmartCommit": false,
    "python.terminal.activateEnvironment": false,
    "redhat.telemetry.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "shellcheck.enable": false,
    "syncSettings.additionalFiles": [
        "~/.config/VSCodium/User/vsicons-custom-icons/file_type_shell_template.svg",
        "~/.config/VSCodium/User/vsicons-custom-icons/folder_type_ansible.svg",
        "~/.config/VSCodium/User/vsicons-custom-icons/folder_type_ansible_opened.svg"
    ],
    "syncSettings.resources": [
        "extensions",
        "keybindings",
        "settings"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "vsicons.associations.files": [
        {
            "icon": "shell_template",
            "format": "svg",
            "extensions": [
                ".shtpl"
            ]
        }
    ],
    "vsicons.associations.folders": [
        {
            "icon": "ansible",
            "format": "svg",
            "extensions": [
                ".ansible",
                "ansible"
            ]
        }
    ],
    "vsicons.customIconFolderPath": "/home/conner/.config/VSCodium/User",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.editor.openPositioning": "last",
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none"
}
