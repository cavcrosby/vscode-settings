    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.minimap.enabled": false,
    "files.associations": {
        "**/handlers/**/**/*.yml": "ansible",
        "**/playbooks/*.yml": "ansible",
        "**/playbooks/tasks/*.yml": "ansible",
        "**/roles/**/**/*.yml": "ansible",
        "*.yml.shtpl": "yaml"
    },
    "git.enableCommitSigning": true,
    "git.inputValidation": true,
    "git.suggestSmartCommit": false,
    "python.terminal.activateEnvironment": false,
    "redhat.telemetry.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "syncSettings.additionalFiles": [
        "~/.config/VSCodium/User/vsicons-custom-icons/file_type_shell_template.svg"
    ],
    "syncSettings.resources": [
        "extensions",
        "keybindings",
        "settings"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "todo-tree.general.tags": [
        "BUG",
        "FIXME",
        "HACK",
        "MONITOR",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "vsicons.associations.files": [
        {
            "extensions": [
                ".shtpl"
            ],
            "format": "svg",
            "icon": "shell_template"
        }
    ],
    "vsicons.associations.folders": [
        {
            "extensions": [
                ".jenkins"
            ],
            "format": "svg",
            "icon": "jenkins"
        }
    ],
    "vsicons.customIconFolderPath": "/home/conner/.config/VSCodium/User",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none"
}
